<%= simple_form_for(@file_ops_audit) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :group_id %>
    <%= f.input :format_id %>
    <%= f.input :encrypted_at %>
    <%= f.input :uploaded_at %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
