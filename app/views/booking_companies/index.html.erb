<h1>Listing booking_companies</h1>

<table class="table table-striped">
  <tr>
    <th>Company name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @booking_companies.each do |booking_company| %>
  <tr>
    <td><%= booking_company.company_name %></td>
    <td><%= link_to 'Show', booking_company %></td>
    <td><%= link_to 'Edit', edit_booking_company_path(booking_company) %></td>
    <td><%= link_to 'Destroy', booking_company, method: :delete, data: { confirm: 'Are you sure?' }, :class => "btn btn-danger" %></td>
  </tr>
<% end %>
</table>

