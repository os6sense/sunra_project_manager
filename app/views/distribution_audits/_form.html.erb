<%= simple_form_for(@distribution_audit) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :group_id %>
    <%= f.input :format_id %>
    <%= f.input :email_address %>
    <%= f.input :emailed_at %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
